{
    "name": "codex/addon-phpdoc",
    "type": "codex-addon",
    "description": "The Phpdoc addon provides Codex the means to parse the phpdoc generated xml file and integrate documenation in a user-friendly way.",
    "keywords": ["laravel", "documentation", "docs", "markdown", "codex", "phpdoc"],
    "homepage": "http://codex-project.ninja",
    "authors": [{"name": "Robin Radic", "email": "robin@radic.nl"}],
    "license": "MIT",
    "require": {
        "codex/composer-plugin": "~2.0",
        "codex/core": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Codex\\Addon\\Phpdoc\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0-beta2"
        }
    }
}